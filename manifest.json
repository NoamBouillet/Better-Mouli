{
  "manifest_version": 3,
  "name": "Better Moulinette",
  "version": "1.0",
  "description": "A better my.epitech.eu visual",
  "permissions": ["storage"],
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": [
    "https://my.epitech.eu/*",
    "https://api.epitest.eu/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://my.epitech.eu/*"],
      "js": ["inject.js"]
    }
  ]
}
